<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.santha.ddd.users.domain.registrations.UserRegistration" table="user_registrations">
        <id name="id" type="com.santha.ddd.users.domain.registrations.UserRegistrationId"  access="field">
            <column name="id" />
        </id>
        <property name="username" type="java.lang.String" access="field">
            <column name="username" />
        </property>
    </class>
</hibernate-mapping>
